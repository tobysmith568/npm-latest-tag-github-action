{
  "name": "npm-publish-latest-tag",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/npm-publish-latest-tag/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/npm-publish-latest-tag",
        "main": "packages/npm-publish-latest-tag/src/index.ts",
        "tsConfig": "packages/npm-publish-latest-tag/tsconfig.lib.json",
        "assets": ["packages/npm-publish-latest-tag/*.md", "LICENSE.md"],
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs npm-publish-latest-tag {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/npm-publish-latest-tag/**/*.ts"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/npm-publish-latest-tag/jest.config.ts",
        "passWithNoTests": false
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    }
  },
  "tags": []
}
