{
  "name": "github-action",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/github-action/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/github-action",
        "main": "packages/github-action/src/index.ts",
        "tsConfig": "packages/github-action/tsconfig.lib.json",
        "externalDependencies": "none",
        "generatePackageJson": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/github-action/**/*.ts"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/github-action/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    }
  },
  "tags": []
}
